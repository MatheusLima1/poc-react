{
    "build": {
      "development": {
        "android": {
          "releaseChannel": "development",
          "buildType": "apk",
          "distribution": "internal",
          "developmentClient": true
        }
      },
      "production-internal": {
        "android": {
          "releaseChannel": "production",
          "developmentClient": true,
          "gradleCommand": ":app:assembleRelease",
          "distribution": "internal"
        }
      },
      "production": {
        "android": {
          "releaseChannel": "production",
          "gradleCommand": ":app:assembleRelease"
        }
      }
    }
  }
  