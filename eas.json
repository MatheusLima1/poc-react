{
  "build": {
    "development": {
      "releaseChannel": "development",
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "releaseChannel": "release",
      "android": {
        "gradleCommand": ":app:bundleRelease"
      }
    },
    "production": {
      "android": {
        "releaseChannel": "master",
        "gradleCommand": ":app:bundleRelease"
      }
    }
  },
  "submit":{
    "production": {
      "android": {
        "releaseStatus": "draft",
        "track": "internal"
      }
    }
  }
}